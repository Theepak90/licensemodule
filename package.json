{
  "name": "torro-license-manager",
  "version": "1.0.0",
  "description": "License Management Dashboard for Torro Platform",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server.js",
    "daemon": "node scripts/start-daemon.js",
    "daemon:start": "node scripts/daemon-control.js start",
    "daemon:stop": "node scripts/daemon-control.js stop",
    "daemon:status": "node scripts/daemon-control.js status",
    "daemon:rotate": "node scripts/daemon-control.js rotate",
    "daemon:reset": "node scripts/daemon-control.js reset",
    "enhanced-daemon": "node scripts/enhanced-daemon-control.js start",
    "enhanced-daemon:start": "node scripts/enhanced-daemon-control.js start",
    "enhanced-daemon:stop": "node scripts/enhanced-daemon-control.js stop",
    "enhanced-daemon:status": "node scripts/enhanced-daemon-control.js status",
    "enhanced-daemon:rotate": "node scripts/enhanced-daemon-control.js rotate",
    "enhanced-daemon:reset": "node scripts/enhanced-daemon-control.js reset",
    "enhanced-daemon:enable-hash": "node scripts/enhanced-daemon-control.js enable-hash",
    "enhanced-daemon:disable-hash": "node scripts/enhanced-daemon-control.js disable-hash",
    "enhanced-daemon:validate-hash": "node scripts/enhanced-daemon-control.js validate-hash",
    "enhanced-daemon:change-password": "node scripts/enhanced-daemon-control.js change-password",
    "config:setup": "node scripts/setup-dynamic-config.js setup",
    "config:interactive": "node scripts/setup-dynamic-config.js interactive",
    "config:show": "node scripts/setup-dynamic-config.js show",
    "config:validate": "node scripts/setup-dynamic-config.js validate",
    "start:validated": "node scripts/validate-config.js && node server.js",
    "setup": "node scripts/setup.js",
    "validate": "node scripts/validate-config.js",
    "docker:build": "docker build -t torro-license-manager .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["license", "management", "dashboard", "torro"],
  "author": "Torro Platform",
  "license": "MIT"
}

